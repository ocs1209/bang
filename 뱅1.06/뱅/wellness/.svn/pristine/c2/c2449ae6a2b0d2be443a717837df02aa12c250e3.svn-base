.ani_zzz>div{-webkit-animation: zzz 2s infinite, zzz-alpha 2s infinite; animation: zzz 2s infinite, zzz-alpha 2s infinite;-webkit-transform-origin: 50% 0%;transform-origin: 50% 0%;transform: scale(.5); -webkit-transform: scale(.5); position: relative;}

@-webkit-keyframes zzz-alpha{
	0%{opacity:0;}
    50%{opacity:1;}
    80%{opacity:1;}
    100%{opacity:0;} 
}

@keyframes zzz-alpha{
	0%{opacity:0;}
    50%{opacity:1;}
    80%{opacity:1;}
    100%{opacity:0;} 
}

@-webkit-keyframes zzz{
	0%{transform: scale(.5) translateY(50px); -webkit-transform: scale(.5) translateY(50px);}
    50%{transform: scale(1) translateY(0px); -webkit-transform: scale(1) translateY(0px);}
    80%{transform: scale(1) translateY(0px); -webkit-transform: scale(1) translateY(0px);}
    100%{transform: scale(1) translateY(-10px); -webkit-transform: scale(1) translateY(-10px);} 
}

@keyframes zzz {
	0%{transform: scale(.5) translateY(50px); -webkit-transform: scale(.5) translateY(50px);}
    50%{transform: scale(1) translateY(0px); -webkit-transform: scale(1) translateY(0px);}
    80%{transform: scale(1) translateY(0px); -webkit-transform: scale(1) translateY(0px);}
    100%{transform: scale(1) translateY(-10px); -webkit-transform: scale(1) translateY(-10px);} 
}

