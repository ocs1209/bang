.img_fire_bg>div:nth-child(1){-webkit-animation-delay: 100ms; animation-delay: 100ms;}
.img_fire_bg>div:nth-child(2){-webkit-animation-delay: 200ms; animation-delay: 200ms;}

.cal_day .img_fire_bg>div{ display: block;width: 294px;height: auto;margin: 0 auto;transform-origin: 50% 92%;-webkit-transform-origin: 50% 92%;}
.cal_day .img_fire_bg>div.ani{animation-duration: 500ms; -webkit-animation-duration: 500ms; animation-iteration-count: infinite; -webkit-animation-iteration-count: infinite;}
.cal_day .lg .img_fire_bg>div{transform: scale(1); -webkit-transform: scale(1.0); animation-name: fire_lg; -webkit-animation-name: fire_lg;}
.cal_day .md .img_fire_bg>div{transform: scale(.8); -webkit-transform: scale(.8); animation-name: fire_md; -webkit-animation-name: fire_md;}
.cal_day .sm .img_fire_bg>div{transform: scale(.6); -webkit-transform: scale(.6);	animation-name: fire_sm; -webkit-animation-name: fire_sm;}
/**/

@keyframes fire_lg {
	0%{transform: scale(1.0); -webkit-transform: scale(1.0);}
    50%{transform: scale(.9); -webkit-transform: scale(.9);}
    100%{transform: scale(1.0); -webkit-transform: scale(1.0);}
}

@keyframes fire_md {
	0%{transform: scale(.8); -webkit-transform: scale(.8);}
    50%{transform: scale(.7); -webkit-transform: scale(.7);}
    100%{transform: scale(.8); -webkit-transform: scale(.8);}
}

@keyframes fire_sm {
	0%{transform: scale(.6); -webkit-transform: scale(.6);}
    50%{transform: scale(.5); -webkit-transform: scale(.5);}
    100%{transform: scale(.6); -webkit-transform: scale(.6);}
}

@-webkit-keyframes fire_lg {
	0%{transform: scale(1); -webkit-transform: scale(1);}
    50%{transform: scale(.9); -webkit-transform: scale(.9);}
    100%{transform: scale(1); -webkit-transform: scale(1);}
}

@-webkit-keyframes fire_md {
	0%{transform: scale(.8); -webkit-transform: scale(.8);}
    50%{transform: scale(.7); -webkit-transform: scale(.7);}
    100%{transform: scale(.8); -webkit-transform: scale(.8);}
}

@-webkit-keyframes fire_sm {
	0%{transform: scale(.6); -webkit-transform: scale(.6);}
    50%{transform: scale(.5); -webkit-transform: scale(.5);}
    100%{transform: scale(.6); -webkit-transform: scale(.6);}
}


