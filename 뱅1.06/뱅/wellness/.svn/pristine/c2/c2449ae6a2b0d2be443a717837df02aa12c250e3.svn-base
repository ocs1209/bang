
@charset "utf-8";

body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, textarea, p, blockquote, th, td, input, select, textarea, button {margin:0; padding:0;}
fieldset, img { border:0 none;}
dl, ul, ol, menu, li { list-style:none;}
h1, h2, h3 {font-weight: normal;}
blockquote, q { quotes:none;}
blockquote:before, blockquote:after, q:before, q:after{ content:'';content:none;}
table{border-collapse:collapse;border-spacing:0;}
body { -webkit-text-size-adjust:none; font-family:'NanumGothic', '나눔고딕';  font-size:14px;} /* position:relative; */
input, select, textarea, button { font-size:100%; vertical-align:middle;}
input[type='text'], input[type='password'], input[type='submit'], input[type='search'], input[type='file']{ -webkit-appearance:none; border-radius:0;}
input[readonly]{border:none;background:none;}
/* input:checked[type='checkbox']{-webkit-appearance:checkbox; background-color:#666;} */
button,input[type='button'], input[type='submit'], input[type='reset'] {-webkit-appearance:button; border-radius:0;}
input, textarea{outline:none;}
button { border:0 none; background-color:transparent; cursor:pointer;}
html, body{position: relative; width: 100%; height: 100%; overflow: hidden;}
a { color:#222222;text-decoration:none;}
a:active, a:hover { text-decoration:none;}
address, caption, cite, code, dfn, var{font-style:normal; font-weight:normal;}
em { font-style:normal;}


/* 우측 메뉴 */
.open #wrap{right: 243px;}
#wrap{display: box;display: -webkit-box;position: relative;height: 100%; right:0;box-orient: vertical;-webkit-box-orient: vertical;/*transition: right 300ms ease-out;-webkit-transition: right 300ms ease-out;*/}
.container{display: box; display: -webkit-box;box-orient: vertical;-webkit-box-orient: vertical;background-color:white;position: relative;overflow:auto;-webkit-box-flex: 2;box-flex: 2;}
div.alert>div{display:none;}
div.alert.try>.try{ display: block;}
div.alert.retry>.retry{ display: block;}
div.alert.retry{height:30px;line-height:30px;background-color: #fffdef}
div.alert.retry span:before{position:absolute;content:"";margin: 7px 0 0 -23px;display:inline-block;width:15px; height: 15px;background-image:url("../images/retry_icon.png"); background-repeat: no-repeat; background-size: 15px; }
div.alert.retry button{
	text-shadow: 0 1px 0 #fff;
	background-image: -webkit-linear-gradient(top,#fff 0,#e0e0e0 100%);
	background-image: -webkit-gradient(linear,left top,left bottom,from(#fff),to(#e0e0e0));  
	background-image: linear-gradient(to bottom,#fff 0,#e0e0e0 100%);
	background-image: gradient(linear,left top,left bottom,from(#fff),to(#e0e0e0));  
	background-repeat: repeat-x;
	text-shadow: 0 -1px 0 rgba(0,0,0,.2);
    -webkit-box-shadow: inset 0 1px 0 rgba(255,255,255,.15),0 1px 1px rgba(0,0,0,.075);
    box-shadow: inset 0 1px 0 rgba(255,255,255,.15),0 1px 1px rgba(0,0,0,.075);
    color: #333;
    background-color: #fff;
    display: inline-block;
    padding: 5px 12px;
    font-size: 12px;
    font-weight: 400;
    line-height: 1.42857143;
    text-align: center;
    vertical-align: middle;
    -webkit-user-select: none;
    user-select: none;
    border: 1px solid transparent;
    border-radius: 4px;
    box-sizing: border-box;
    border-color: #ccc;
    position: absolute;
    right: 5px;
    top: 4px;
}

div.alert{position:relative;height: 20px;width: 100%;display: box;display: -webkit-box; box-pack: center;-webkit-box-pack: center;padding: 5px 0 3px 0; background: rgba(255,255,255,0.5); font-size: 12px; line-height: 20px;}
div.alert img{vertical-align: middle;margin: 0px 7px 2px 0;}
div.alert.try span{margin: 0 0 0 20px;}
div.alert .bar{position: absolute;background: #609BC1;text-indent: -9999px; width: 100%; height: 2px;top: 0;left: 0;}
div.alert .m-loadingspinner{position:absolute; top:0;padding:7px 0 0 0;}
div.alert .m-spinner-icon{width: 14px !important; height: 14px !important;border-top-width: 2px !important; border-right-width: 2px !important; border-left-width: 2px !important; border-bottom-width: 2px !important; }
    

